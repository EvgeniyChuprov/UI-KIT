mixin bubble-slider(min, max, step, value)
    .custom-rangeslider
        input(class="custom-rangeInput" title="Percentage" id="range-slider1" type="range" min= min max= max value= value step= step data-tooltip="true" aria-controls="rangeTooltip rangeLabel" aria-live="polite")
        span(class="custom-rangeslider__tooltip") 0
